## ClickHouse connection
CH_HOST=127.0.0.1
CH_PORT=9001
CH_DB=fgis
CH_USER_INGEST=default
CH_PASS_INGEST=

## Optional read-only user
CH_USER_READ=
CH_PASS_READ=

## HTTP(S) proxy for FGIS requests (if required)
HTTP_PROXY=
HTTPS_PROXY=

## Insert tuning
CH_INSERT_BATCH=500
CH_INSERT_RETRIES=3

## HTTP retry/backoff
HTTP_MAX_RETRIES=5
HTTP_BASE_SLEEP=1.0
HTTP_MAX_SLEEP=30.0

## Backend sync tuning
FGIS_RPS=1.0
MIT_ROWS=1000
VRI_ROWS=1000
MIT_SLEEP=0.2
VRI_SLEEP=0.2
VRI_TAIL_DAYS=3
START_DATE=
END_DATE=
SYNC_MIT=1
SYNC_VRI=1
MIT_FETCH_DETAILS=1
VRI_SKIP_EXISTING=1

## Sync schedule
MIT_EVERY_HOURS=3
VRI_SCHEDULED=1
VRI_HOURLY_EVERY_HOURS=1
VRI_HOURLY_DAYS=7
VRI_DAILY_EVERY_HOURS=24
VRI_DAILY_DAYS=30
VRI_WEEKLY_EVERY_HOURS=168
VRI_WEEKLY_DAYS=60
VRI_MONTHLY_EVERY_HOURS=720
VRI_MONTHLY_DAYS=180
